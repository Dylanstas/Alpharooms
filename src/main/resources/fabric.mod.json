{
  "schemaVersion": 1,
  "id": "nohunger",
  "version": "${version}",

  "name": "No Hunger",
  "description": "This mod lets you play without worrying about hunger!\nWhen you eat you replenish HP",
  "authors": [
    "Slainlite",
    "MrIbby"
  ],

  "contact": {
    "homepage": "https://fabricmc.net/",
    "sources": "https://github.com/FabricMC/fabric-example-mod"
  },

  "license": "MIT",
  "icon": "assets/nohunger/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "net.Slainlight.NoHunger.Main"
    ],
    "modmenu": [
      "net.Slainlight.NoHunger.NoHungerModMenu"
    ]
  },
  "mixins": [
    "nohunger.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "*"
  }
}
